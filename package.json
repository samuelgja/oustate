{
  "name": "oustate",
  "version": "1.0.1",
  "author": "samuel.gjabel@gmail.com",
  "description": "ðŸ‘€ Another React state management library",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "react-native": "lib/src/index.ts",
  "homepage": "https://github.com/samuelgjabel/oustate",
  "repository": "https://github.com/samuelgjabel/oustate",
  "sideEffects": false,
  "scripts": {
    "ts:build": "tsc ",
    "build": "bun run build:cjs && bun run build:esm && bun run build:umd && bun run build:types && node scripts/copy-data.mjs",
    "build:cjs": "rollup -c --bundleConfigAsCjs --format=cjs && rm -rf lib/cjs/node_modules && rm -rf lib/cjs/_virtual",
    "build:esm": "rollup -c --bundleConfigAsCjs --format=esm && rm -rf lib/esm/node_modules && rm -rf lib/esm/_virtual",
    "build:umd": "rollup -c --bundleConfigAsCjs --format=umd && rm -rf lib/umd/node_modules && rm -rf lib/umd/_virtual",
    "build:types": "rollup -c --bundleConfigAsCjs --format=types && rm -rf lib/types/index.js",
    "publish:prepare": "node scripts/publish",
    "code-check": "bun run format && bun run typecheck && bun run lint && bun run test",
    "typecheck": "tsc --noEmit",
    "lint": "bun eslint \"packages/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "test": "bun test",
    "test:it": "jest  --env=jsdom --verbose --runInBand",
    "pub": "bun run code-check && bun run test && bun run build && cd lib && npm publish",
    "pub:babel-transform": "bun run code-check && bun run test && cd packages/babel-transform && bun run pub"
  },
  "dependencies": {
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint-react/eslint-plugin": "1.15.1",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "@happy-dom/global-registrator": "^15.11.6",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "^12.1.1",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@stylistic/eslint-plugin-jsx": "^2.9.0",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@testing-library/react": "^16.0.1",
    "@types/bun": "^1.1.11",
    "@types/eslint": "^9.6.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "18.3.0",
    "@types/use-sync-external-store": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "bun-types": "1.1.33",
    "change-case-all": "^2.1.0",
    "eslint": "^9.13.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-plugin-depend": "^0.11.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-perf": "^3.3.3",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-sonarjs": "2.0.4",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-unicorn": "55.0.0",
    "expo-atlas": "^0.3.0",
    "glob": "^11.0.0",
    "globals": "^15.8.0",
    "jest": "^29.2.1",
    "jest-expo": "52.0.0",
    "jsonschema": "^1.4.1",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react-test-renderer": "18.2.0",
    "rollup": "^4.26.0",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.4.7",
    "ts-jest": "^29.2.3",
    "ts-node": "10.9.2",
    "typescript": "5.6.3",
    "typescript-eslint": "^7.17.0"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
