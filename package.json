{
  "name": "oustate",
  "version": "0.1.1",
  "author": "samuel.gjabel@gmail.com",
  "description": "ðŸ‘€ Another React state management library",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "react-native": "lib/src/index.ts",
  "homepage": "https://github.com/samuelgjabel/oustate",
  "repository": "https://github.com/samuelgjabel/oustate",
  "sideEffects": false,
  "scripts": {
    "ts:build": "tsc ",
    "build": "bun run build:cjs && bun run build:esm && bun run build:umd && bun run build:types && node scripts/copy-data.mjs",
    "build:cjs": "rollup -c --bundleConfigAsCjs --format=cjs && rm -rf lib/cjs/node_modules && rm -rf lib/cjs/_virtual",
    "build:esm": "rollup -c --bundleConfigAsCjs --format=esm && rm -rf lib/esm/node_modules && rm -rf lib/esm/_virtual",
    "build:umd": "rollup -c --bundleConfigAsCjs --format=umd && rm -rf lib/umd/node_modules && rm -rf lib/umd/_virtual",
    "build:types": "rollup -c --bundleConfigAsCjs --format=types && rm -rf lib/types/index.js",
    "publish:prepare": "node scripts/publish",
    "code-check": "bun run format && bun run typecheck && bun run lint",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "test": "jest __tests__/*.test.* --env=jsdom --verbose --runInBand",
    "test:it": "jest  --env=jsdom --verbose --runInBand",
    "pub": "bun run code-check && bun run test && bun run build && cd lib && npm publish",
    "pub:babel-transform": "bun run code-check && bun run test && cd packages/babel-transform && bun run pub"
  },
  "dependencies": {
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "7.18.6",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-typescript": "^9.0.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.13",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-syntax-highlighter": "13.5.2",
    "@types/react-table": "7.7.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-virtualized-auto-sizer": "1.0.1",
    "@types/react-window": "1.8.5",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "babel-jest": "^29.0.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^7.4.0",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-unicorn": "40.1.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "immer": "^9.0.16",
    "jest": "29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "metro-react-native-babel-preset": "^0.73.3",
    "prettier": "2.7.1",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^3.1.0",
    "rollup-plugin-esbuild": "^4.10.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
